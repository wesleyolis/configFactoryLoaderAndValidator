{
  "name": "platform-loader",
  "version": "1.0.5",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "tsct": "tsc -v;",
    "tsc-test": "tsc -p tsconfig-test.json",
    "copy-d-ts": "find ./src -name  '*.d.ts' | cut -d '/' -f3- | xargs -I {} cp './lib/{}' './lib/{}'",
    "copy-js": "rm -rf lib && cp -r src/ lib/; find ./lib -name '*.ts' -type f | xargs rm",
    "copy-js-test": "rm -rf lib-test && cp -r test/ lib-test/; find ./lib-test -name '*.ts' -type f | xargs rm",
    "build": "npm run copy-js && tsc",
    "build-test": "npm run copy-js-test && tsc -p tsconfig-test.json",
    "build-all": "npm run build && npm run build-test",
    "test": "mocha --recursive 'lib-test/**/*.spec.js'",
    "test-coverage": "nyc --all mocha --recursive 'lib-test/**/*.spec.js'",
    "build-run-test": "npm run build-test && npm run test",
    "build-run-test-coverage": "npm run build-test && npm run test-coverage",
    "build-all-run-test": "npm run build-all && npm run test",
    "build-all-run-test-coverage": "npm run build-all && npm run test-coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/chai": "^3.5.0",
    "@types/joi": "^13.0.8",
    "@types/lodash": "^4.14.121",
    "@types/mocha": "^2.2.48",
    "@types/mongoose": "3.8.x",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^1.16.36",
    "@types/sinon-chai": "^2.7.31",
    "@types/ssh2": "^0.5.35",
    "@types/ssh2-streams": "^0.1.2",
    "@types/verror": "^1.10.3",
    "bluebird": "^3.5.1",
    "buffer-equal-constant-time": "^1.0.1",
    "chai": "^3.5.0",
    "config": "^1.29.4",
    "joi": "^13.4.0",
    "lodash": "^4.17.11",
    "moch": "0.0.1",
    "mocha": "^2.5.3",
    "mongo-in-memory": "0.0.5",
    "mongoose": "^3.8.40",
    "nyc": "^11.7.3",
    "proxyquire": "^1.7.9",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "ssh2": "^0.5.2",
    "verror": "^1.10.0"
  }
}
